package proGramers;

public class 모의고사 {
	public static void main(String[] args) {
		int[] answers = new int[10000];
		int[] answers1 = new int[10000];
		int[] answers2 = new int[10000];
		int[] answers3 = new int[10000];
		int[] rand = {3,1,2,4,5};
		int ans = 0;

		int ans1 = 0;
		for(int i = 0; i <answers.length;i++)
		{
			ans++;
			if(ans == 6){ans = 1;}
			answers1[i]= ans;
			
			if(i%2 ==0){answers2[i] = 2;}
			else if(ans == 2){answers2[i] = ans+1;}
			else {answers2[i] = ans;}
			
			answers3[i] = rand[ans1];
			if(i%2 ==1){ans1++;}
		}
	}
	
}
class Solution {
    public int[] solution(int[] answers) {
        int[] answer = {};
        return answer;
    }
}