package proGramers;

public class 삼진법 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Solution b = new Solution();
		long a = 0;
		a = b.solution(3, 4);
		System.out.println(a);
	}

}
class Solution {
    public long solution(int a, int b) {
        Solution x = new Solution();
    	long answer = 0;
        if(a>b){answer =x.sumAll(b, a);}
        else if(a<b){answer =x.solution(a, b);}
        else {answer = x.sumAll(a, b);}
        
        return answer;
    }
    private long sumAll(int a,int b)
    {
    	long sum =0;
    	for(int i=a;i<=b;i++)
    	{
    		sum += i;
    	}
    	return sum;
    }
}