        if(a>b){
        	for(int i=a;i<=b;i++)
        	{
        		answer += i;
        	}
        	}
        else if(a<b){
        	for(int i=a;i<=b;i++)
        	{
        		answer += i;
        	}
        }
        else {for(int i=a;i<=b;i++)
    	{
    		answer += i;
    	}}
        
	}


    private long sumAll(int a,int b)
    {
    	long sum =0;
    	for(int i=a;i<=b;i++)
    	{
    		sum += i;
    	}
    	return sum;
    }
}