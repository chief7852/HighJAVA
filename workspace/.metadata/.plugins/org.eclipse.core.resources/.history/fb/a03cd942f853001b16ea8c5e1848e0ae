package proGramers;

public class 모의고사 {
	public static void main(String[] args) {
		int[] answers = {1,3,2,4,2};

		int[] rand = {3,1,2,4,5};
		int ans = 0;
		int ans1 = 0;
		int ans2 = 1;
		
		int score1=0;
		int score2=0;
		int score3=0;
		
		for(int i = 0; i <answers.length;i++)
		{
			ans++;
			
			if(ans2 == 6){ans2 = 1;}
			if(ans == 6){ans = 1;}
			if(ans==answers[i]){score1++;}
			
			
			if(i%2 ==0&&answers[i]==2){score2++;}
			if(ans2 ==2){ans2++;}
			if(i%2 == 1&&answers[i]==ans2){score2++;}
			if(i%2 == 1){ans2++;}
			
			if(ans1 == 6){ans =1;}
			if(answers[i] == rand[ans1]){score3++;}
			if(i%2 ==1){ans1++;}
		}
		System.out.print("1번"+score1);
		System.out.print("2번"+score2);
		System.out.print("3번"+score3);
	}
	
}
class Solution {
    public int[] solution(int[] answers) {
        int[] answer = {};
        return answer;
    }
}