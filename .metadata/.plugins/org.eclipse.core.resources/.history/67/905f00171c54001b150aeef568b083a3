package proGramers;

public class 삼진법 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a = 3;
		int b = 5;
		long answer = 0;
        if(a>b){
        	for(int i=a;i<=b;i++)
        	{
        		answer += i;
        	}
        	}
        else if(a<b){
        	for(int i=a;i<=b;i++)
        	{
        		answer += i;
        	}
        }
        else {for(int i=a;i<=b;i++)
    	{
    		answer += i;
    	}}
        System.out.println(answer);
	}


    
}

class Solution {
    public long solution(int a, int b) {
        long answer = 0;
        Solution sol = new Solution();
        if(a>b){
        	sol.sumAll(b, a);
        	}
        else if(a<b){
        	sol.sumAll(a, b);        
        }
        else {answer = a;}   
        return answer;
    }
    private long sumAll(int a,int b)
    {
        long answer =0;
    	for(int i=a;i<=b;i++)
    	{
    		answer += i;
    	}
        return answer;
    }
}