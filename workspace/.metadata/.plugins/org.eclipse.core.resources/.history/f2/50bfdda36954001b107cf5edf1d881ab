package proGramers;

import java.util.ArrayList;

public class 삼진법뒤집기 {

	public static void main(String[] args) {
		int n = 125;
		ArrayList<Integer> list = new ArrayList<>();	
		
		while(n<3)
		{
			list.add(n%3);
			if(n>3){n=n/3;}
		}
		for(int a : list)
		{
			System.out.println(a);
		}
		System.out.println(list.isEmpty());
//		for(int i = 1; i <= n;i++)
//		{
//			if(i%3==0)
//			{
//				
//			}
//		}
	}

}
