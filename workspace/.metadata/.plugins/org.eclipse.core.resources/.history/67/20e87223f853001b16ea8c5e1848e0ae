package proGramers;

public class 모의고사 {
	public static void main(String[] args) {
		int[] answers = new int[10000];
		int[] answers1 = new int[10000];
		int[] answers2 = new int[10000];
		int[] answers3 = new int[10000];
		int[] rand = {3,1,2,4,5};
		int ans = 0;
		int ans1 = 0;
		int ans2 = 1;
		
		int score1=0;
		int score2=0;
		int score3=0;
		
		for(int i = 0; i <answers.length;i++)
		{
			ans++;
			if(ans == 6){ans = 1;}
			if(ans==answers[i]){score1++;}
			
			if(i%2 ==0&&answers[i]==2){score2++;}
			if(ans == 2&&answers[i]==3){score2++;}
			if(i%2 == 1&&ans !=2 ){
			
			if(ans1 == 6){ans =1;}
			answers3[i] = rand[ans1];
			if(i%2 ==1){ans1++;}
		}
	}
	
}
